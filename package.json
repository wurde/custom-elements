{
  "name": "themebuilder",
  "version": "2.2.0",
  "description": "Customize all design aspects of your React components.",
  "main": "index.js",
  "private": true,
  "scripts": {
    "start": "cross-env NODE_ENV=development concurrently --kill-others npm:start:client npm:start:electron",
    "start:client": "npm run start --prefix client",
    "start:electron": "wait-on http://localhost:3000 && electron index.js --no-sandbox",
    "prebuild": "rimraf client/build",
    "build": "npm run build --prefix client",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "author": "Andy Bettisworth",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/wurde/themebuilder.git"
  },
  "bugs": {
    "url": "https://github.com/wurde/themebuilder/issues"
  },
  "homepage": "https://github.com/wurde/themebuilder#readme",
  "build": {
    "appId": "com.wurde.themebuilder",
    "productName": "Theme Builder",
    "target": "snap"
  },
  "devDependencies": {
    "concurrently": "^5.0.0",
    "cross-env": "^6.0.3",
    "cypress": "^3.4.1",
    "electron": "^6.0.10",
    "electron-builder": "^21.2.0",
    "rimraf": "^3.0.0",
    "wait-on": "^3.3.0"
  }
}
